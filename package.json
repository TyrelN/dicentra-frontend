{
  "name": "frontend",
  "version": "0.0.1",
  "description": "Apply to home or help animals, learn about proper care and donate to the Nicola Valley Animal Rescue located in Merritt, British Columbia.",
  "productName": "Nicola Valley Animal Rescue",
  "author": "tyreln <tyrelnarciso@hotmail.com>",
  "private": true,
  "scripts": {
    "lint": "eslint --ext .js,.vue ./",
    "test": "echo \"See package.json => scripts for available tests.\" && exit 0",
    "dev": "quasar dev",
    "build": "quasar build",
    "deploy": "vercel",
    "build:pwa": "quasar build -m pwa",
    "test:unit": "jest --updateSnapshot",
    "test:unit:ci": "jest --ci",
    "test:unit:coverage": "jest --coverage",
    "test:unit:watch": "jest --watch",
    "test:unit:watchAll": "jest --watchAll",
    "serve:test:coverage": "quasar serve test/jest/coverage/lcov-report/ --port 8788",
    "concurrently:dev:jest": "concurrently \"quasar dev\" \"jest --watch\""
  },
  "dependencies": {
    "@quasar/cli": "^1.2.2",
    "@quasar/extras": "^1.12.2",
    "axios": "^0.21.1",
